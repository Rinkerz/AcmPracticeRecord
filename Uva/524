void dfs(int cur)
{
    if(cur==n&&isp[ans[0]+ans[cur-1]]) {for(int i=0;i!=n;i++) cout<<ans[i]<<" ";cout<<endl;}
    else for(int i=2;i<=n;i++)
        if(!used[i]&&isp[i+ans[cur-1]]){
            ans[cur]=i;
            used[i]=1;
            dfs(cur+1);
            used[i]=0;
        }
}

int main()
{
    int cnt=0;
    while(cin>>n)
    {
        cout<<"Case "<<++cnt<<":"<<endl;
        memset(used,0,sizeof(used));memset(ans,0,sizeof(ans));ans[0]=1;
        dfs(1);
        cout<<endl;
    }
}

